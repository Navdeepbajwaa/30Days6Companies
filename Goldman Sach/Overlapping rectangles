class Solution {
  public:
    int doOverlap(int L1[], int R1[], int L2[], int R2[]) {
        //if one is on left side of other
        if(L1[0]>R2[0]||L2[0]>R1[0]){return false;}//0=x cordinates
        //if one is above of other
        if(L1[1]<R2[1]||L2[1]<R1[1]){return false;}//1= y cordinate
        return true;
    }
};
